{
  "version": 2,
  "rewrites": [
    {
      "source": "/web/(.*)",
      "destination": "/web/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/src/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VERCEL": "1"
  },
  "regions": ["sin1", "hkg1"],
  "functions": {
    "src/index.js": {
      "maxDuration": 60,
      "memory": 1024
    }
  }
}
